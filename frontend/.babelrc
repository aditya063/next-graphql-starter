{
  "env": {
    "development": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false,
            "minify": true,
            "pure": true,
            "transpileTemplateLiterals": true
          }
        ],
        [
          "import",
          {
            "libraryName": "antd",
            "style": "css"
          }
        ]
        // "add-react-displayname"
      ]
    },
    "production": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false,
            "minify": true,
            "pure": true,
            "transpileTemplateLiterals": true
          }
        ],
        [
          "import",
          {
            "libraryName": "antd",
            "style": "css"
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {
              "modules": "commonjs"
            }
          }
        ]
      ],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false,
            "minify": true,
            "pure": true,
            "transpileTemplateLiterals": true
          }
        ],
        [
          "import",
          {
            "libraryName": "antd",
            "style": "css"
          }
        ]
      ]
    }
  }
}
