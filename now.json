{
  "name": "next-graphql-starter",
  "version": 2,
  "builds": [
    { "src": "frontend/package.json", "use": "@now/next" },
    { "src": "backend/package.json", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "frontend/$1" },
    { "src": "/static/(.*)", "dest": "frontend/static/$1" }
  ]
}
