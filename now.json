{
  "name": "next-graphql-starter",
  "version": 2,
  "builds": [
    { "src": "backend/package.json", "use": "@now/node" },
    { "src": "frontend/package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "backend/$1" },
    { "src": "/(.*)", "dest": "frontend/$1" }
  ]
}
